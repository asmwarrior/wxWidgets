<?xml version="1.0" ?>
<makefile>

    <if cond="FORMAT=='autoconf'">
        <option name="wxUSE_TOMCRYPT"/>
        <set var="LIB_TOMCRYPT">
            <if cond="wxUSE_TOMCRYPT=='builtin'">
                wxtomcrypt$(WXDEBUGFLAG)$(WX_LIB_FLAVOUR)-$(WX_RELEASE)$(HOST_SUFFIX)
            </if>
        </set>
    </if>
    <if cond="FORMAT!='autoconf'">
        <set var="wxUSE_TOMCRYPT">builtin</set>
        <set var="LIB_TOMCRYPT">
            <if cond="wxUSE_TOMCRYPT=='builtin'">
                wxtomcrypt$(WXDEBUGFLAG)$(HOST_SUFFIX)
            </if>
        </set>
    </if>
    <set var="INC_TOMCRYPT">
        <if cond="wxUSE_TOMCRYPT=='builtin'">$(TOP_SRCDIR)src/libtomcrypt/src/headers</if>
    </set>

    <lib id="wxtomcrypt" template="3rdparty_lib"
         cond="wxUSE_TOMCRYPT=='builtin' and BUILDING_LIB=='1'">
        <dirname>$(LIBDIRNAME)</dirname>
        <define>LTC_NOTHING</define>
        <define>LTC_HMAC</define>
        <define>LTC_SHA1</define>
        <define>LTC_RIJNDAEL</define>
        <define>LTC_CTR_MODE</define>
        <cflags-borland>
            -w-8004 -w-8008 -w-8012 -w-8057 -w-8066
        </cflags-borland> 
